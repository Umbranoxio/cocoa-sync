<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0-windows7.0</TargetFramework>
	<LangVersion>latest</LangVersion>
    <RootNamespace>CocoaSync</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <StartupObject>CocoaSync.Program</StartupObject>
    <PackageProjectUrl>https://github.com/umbranoxio/cocoa-sync</PackageProjectUrl>
    <RepositoryUrl>https://github.com/umbranoxio/cocoa-sync</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <FileVersion>1.0.0.0</FileVersion>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <Version>1.0.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Spectre.Console" Version="0.48.0" />
  </ItemGroup>

</Project>
